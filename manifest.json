{
  "manifest_version" : 2,
  "name": "Find Unfollowers",
  "description": "provides list of people who don't follow you back",
  "version": "1.0",
  "permissions": [
      "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "action": {
    "default_icon": "assets/icon-196x196.png"
  },
  "icons":{
    "196": "assets/icon-196x196.png",
    "228": "assets/icon-228x228.png"
  }
}